{
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,
  "[bicep]": {
    "editor.defaultFormatter": "ms-azuretools.vscode-bicep"
  },
  "jest.virtualFolders": [
    {
      "name": "wfapi-unit-tests",
      "rootPath": "wfapi",
      "jestCommandLine": "npm run test --"
    },
    {
      "name": "wfapi-integration-tests",
      "rootPath": "wfapi",
      "runMode": "on-demand",
      "jestCommandLine": "npm run test:integration --"
    },
    {
      "name": "wfphotos-unit-tests",
      "rootPath": "wfphotos",
      "jestCommandLine": "npm run test --"
    },
    {
      "name": "wfphotos-integration-tests",
      "rootPath": "wfphotos",
      "runMode": "on-demand",
      "jestCommandLine": "npm run test:integration --"
    }
  ],
  "eslint.lintTask.enable": true,
  "typescript.tsdk": "node_modules/typescript/lib",
  "remote.localPortHost": "allInterfaces"
}
